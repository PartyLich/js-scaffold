{
  "ecmaVersion": 8,
  "libs": [
    "browser",
    "react"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**",
    "/**/*.test.js"
  ],
  "plugins": {
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
